{
  "name": "Duckfinder",
  "version": "1.2",
  "manifest_version": 2,
  "description": "A Chrome extension that replaces the word 'duck' with 'GOOSE'.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
